{
  "name": "framework",
  "version": "0.0.1",
  "description": "framework",
  "scripts": {
    "start": "node server.js",
    "develop": "nodemon server.js",
    "debug": "nodemon --inspect=9231 server.js",
    "standard": "standard --fix"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "watch": [
      "app_modules",
      "routes"
    ],
    "events": {
      "restart": "fuser -k 7777/tcp;",
      "start": "fuser -k 7777/tcp;",
      "crash": "fuser -k 7777/tcp;"
    },
    "delay": "1500"
  },
  "pre-commit": [
    "standard"
  ],
  "standard": {
    "ignore": [
      "/lib/"
    ]
  },
  "devDependencies": {
    "jsdoc": "^3.6.6",
    "jsdoc-http-plugin": "^0.3.2",
    "nodemon": "^2.0.2",
    "pre-commit": "^1.2.2",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "@algolia/recommend": "^4.16.0",
    "@elastic/elasticsearch": "^8.5.0",
    "@google-cloud/translate": "^8.1.0",
    "@google/generative-ai": "^0.1.3",
    "@langchain/openai": "^0.1.3",
    "@pinecone-database/pinecone": "^2.2.1",
    "algoliasearch": "^4.16.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "elastic-apm-node": "^3.47.0",
    "express": "^4.18.2",
    "google-trends-api": "^4.9.2",
    "helmet": "^6.0.1",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.0",
    "langchain": "^0.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^6.8.1",
    "mongoose-timestamp-plugin": "^0.0.1",
    "multer": "^1.4.5-lts.1",
    "node-vault": "^0.9.22",
    "openai": "^4.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "redis": "^3.1.2",
    "uuidv4": "^6.2.13",

    "zod": "^3.23.8"
  }
}

